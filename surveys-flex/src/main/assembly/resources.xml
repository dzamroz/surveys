<?xml version="1.0"  encoding="UTF-8" ?>
<assembly>
	<!--
		Takes files generated by flexmojos:wrapper and puts them into a *.zip file.
	-->
	<id>resources</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>
				${project.build.directory}/wrapper
			</directory>
			<outputDirectory>/</outputDirectory>
			<excludes>
				<exclude>
					template.zip
				</exclude>
			</excludes>
		</fileSet>
	</fileSets>
</assembly>
